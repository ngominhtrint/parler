<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<h2>Users</h2>
  <table class="table">
    <thead class="thead thead-default"> 
      <tr>
        <th>Name</th>
        <th>Email</th>
      </tr>
    </thead>

    <tbody>
      <% User.all.each do |user| %> 
        <tr>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

<%= link_to "Register", new_user_path %>
